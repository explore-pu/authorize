{
    "name": "pucoder/authorize",
    "description": "authorize extension of laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "authorize"],
    "homepage": "https://github.com/pucoder/authorize",
    "license": "MIT",
    "version": "v1.0.0",
    "authors": [
        {
            "name": "xiaozhi",
            "email": "xiaozhi2015@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "pucoder/laravel-admin": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Authorize\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Authorize\\AuthorizeServiceProvider"
            ]
        }
    }
}
